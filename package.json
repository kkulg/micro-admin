{
  "name": "micro-admin",
  "version": "1.0.0",
  "description": "Vue3 + Naive micro front end",
  "main": "index.js",
  "private": true,
  "scripts": {
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkulg/micro-admin.git"
  },
  "keywords": [
    "Vue3",
    "Naive"
  ],
  "author": "kkulg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkulg/micro-admin/issues"
  },
  "homepage": "https://github.com/kkulg/micro-admin#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^17.0.5",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "gitmoji-cli": "^4.7.0",
    "husky": "^7.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
